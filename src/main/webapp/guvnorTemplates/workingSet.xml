<org.drools.guvnor.client.rpc.WorkingSetConfigData>
  <constraints class="linked-list">
    $data:{d|  
    <org.drools.ide.common.client.factconstraints.config.SimpleConstraintConfigurationImpl>
      <args/>
      <constraintName>MandatoryFieldConstraint</constraintName>
      <factType>$d.validFact$</factType>
      <fieldName>$d.factField$</fieldName>
    </org.drools.ide.common.client.factconstraints.config.SimpleConstraintConfigurationImpl>
    <org.drools.ide.common.client.factconstraints.config.SimpleConstraintConfigurationImpl>
      <args>
        <entry>
          <string>matches</string>
          <string>$d.matchesValue$</string>
        </entry>
      </args>
      <constraintName>Matches</constraintName>
      <factType>$d.validFact$</factType>
      <fieldName>$d.factField$</fieldName>
    </org.drools.ide.common.client.factconstraints.config.SimpleConstraintConfigurationImpl>
    }$
  </constraints>
  <validFacts>
    $data:{d|  
    <string>$d.validFact$</string>
    }$
  </validFacts>
  <customForms>
    $data:{d|  
    <org.drools.ide.common.client.factconstraints.customform.predefined.DefaultCustomFormImplementation>
      <width>220</width>
      <height>300</height>
      <factType>$d.validFact$</factType>
      <fieldName>$d.factField$</fieldName>
      <url>$customFormURL$</url>
      <useFormIdForRule>true</useFormIdForRule>
    </org.drools.ide.common.client.factconstraints.customform.predefined.DefaultCustomFormImplementation>
    }$
  </customForms>
</org.drools.guvnor.client.rpc.WorkingSetConfigData>